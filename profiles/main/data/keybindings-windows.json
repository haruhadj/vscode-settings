// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+shift+alt+f9",
        "command": "syncSettings.review"
    },
    {
        "key": "ctrl+shift+alt+f10",
        "command": "syncSettings.viewDifferences"
    },
    {
        "key": "ctrl+shift+alt+f",
        "command": "editor.action.formatDocument.multiple"
    },
    {
        "key": "ctrl+shift+alt+t",
        "command": "workbench.action.createTerminalEditorSide",
        "when": "terminalProcessSupported"
    }
]